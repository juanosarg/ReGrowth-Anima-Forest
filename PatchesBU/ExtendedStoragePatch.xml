<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Extended Storage</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>



				<li Class="PatchOperationAdd"> 
					<xpath>/Defs/ThingDef[defName = "Storage_Basket"]/stuffCategories</xpath>
					<value>
						
						<li>AB_ExoticWooden</li>
						<li>AB_ExoticMetals</li>
					</value>
				</li>
				
				<li Class="PatchOperationAdd"> 
					<xpath>/Defs/ThingDef[defName = "Storage_HazMatContainer"]/stuffCategories</xpath>
					<value>
					
						<li>AB_ExoticMetals</li>
					</value>
				</li>
				
				<li Class="PatchOperationAdd"> 
					<xpath>/Defs/ThingDef[defName = "Storage_Locker"]/stuffCategories</xpath>
					<value>
						<li>AB_ExoticMetals</li>
						<li>AB_ExoticWooden</li>
					</value>
				</li>
				<li Class="PatchOperationAdd"> 
					<xpath>/Defs/ThingDef[defName = "Storage_MedicineCabinet"]/stuffCategories</xpath>
					<value>
						
						<li>AB_ExoticMetals</li>
						<li>AB_ExoticWooden</li>
					</value>
				</li>
				<li Class="PatchOperationAdd"> 
					<xpath>/Defs/ThingDef[defName = "Storage_Skip"]/stuffCategories</xpath>
					<value>
						
						<li>AB_ExoticMetals</li>
					</value>
				</li>
				<li Class="PatchOperationAdd"> 
					<xpath>/Defs/ThingDef[defName = "Storage_TrayRack"]/stuffCategories</xpath>
					<value>
					
						<li>AB_ExoticMetals</li>
						<li>AB_ExoticWooden</li>
					</value>
				</li>
				<li Class="PatchOperationAdd"> 
					<xpath>/Defs/ThingDef[defName = "Storage_WoodenPallet"]/stuffCategories</xpath>
					<value>
						
						<li>AB_ExoticMetals</li>
						<li>AB_ExoticWooden</li>
					</value>
				</li>
				


			</operations>
		</match>
	</Operation>


</Patch>

